%nav.navbar.navbar-default{:role => "navigation"}
  .container-fluid
    %ul.nav.navbar-nav
      %li
        = link_to 'Social network', root_path, class: 'navbar-brand'
      - if user_signed_in?
        %li.active= link_to t('home'), root_path
        %li= link_to t('members'), users_path
    %ul.nav.navbar-nav.navbar-right
      - if user_signed_in?
        %li= link_to t('logout'), destroy_user_session_path, method: :delete
      - else
        %li= link_to t('register'), new_user_registration_path
        %li= link_to t('login'), new_user_session_path
